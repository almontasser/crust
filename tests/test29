extern fn open(pathname: *char, flags: u32): u32;
extern fn read(fd: u32, buf: *char, count: u32): u32;
extern fn write(fd: u32, buf: *char, count: u32): u32;
extern fn close(fd: u32): u32;

let buf: *char;

fn main(): u32 {
  let zin: u32;
  let cnt: u32;

  buf= "                                                             ";
  zin = open("input30", 0);
  if (zin == -1) {
    return 1;
  }
  while ((cnt = read(zin, buf, 60)) > 0) {
    write(1, buf, cnt);
  }
  close(zin);
  return 0;
}
