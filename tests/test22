let a: char; let b: char; let c: char;
let  d: u64; let  e: u64; let  f: u64;
let g: u32; let h: u32; let i: u32;


fn main(): u8 {
  b= 5; c= 7; a= b + c++; printint(a);
  e= 5; f= 7; d= e + f++; printint(d);
  h= 5; i= 7; g= h + i++; printint(g);
  a= b-- + c; printint(a);
  d= e-- + f; printint(d);
  g= h-- + i; printint(g);
  a= ++b + c; printint(a);
  d= ++e + f; printint(d);
  g= ++h + i; printint(g);
  a= b * --c; printint(a);
  d= e * --f; printint(d);
  g= h * --i; printint(g);
  return 0;
}